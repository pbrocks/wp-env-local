{
    "plugins": [
        "./wp-content/plugins/wp-env-local"
    ],
    "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true
    },
    "mappings": {
        "wp-content/plugins": "./wp-content/plugins",
        "wp-content/themes": "./wp-content/themes",
        "/docker-entrypoint-initdb.d": "./docker/mysql"
    },
    "env": {
        "development": {
            "themes": [
                "WordPress/twentytwentyfour"
            ],
            "port": 2193,
            "mysqlPort": 12306
        },
        "tests": {
            "config": {
                "WP_DEBUG_DISPLAY": false
            },
            "port": 3000,
            "mysqlPort": 13306
        }
    }
}
