    "mappings": {
        "wp-content/plugins": "./local/plugins",
        "wp-content/themes": "./local/themes",
        "wp-content/info-passed": "./local/info-to-pass",
        "/docker-entrypoint-initdb.d": "./docker/mysql"
    }
}
