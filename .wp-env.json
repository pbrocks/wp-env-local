{
    "plugins": [
        "."
    ],
    "themes": [
        "WordPress/twentytwentyfour"
    ],
    "port": 5544,
    "env": {
        "tests": {
            "port": 5444
        }
    },
    "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "WP_DEBUG_DISPLAY": true,
        "WP_PERMALINK_STRUCTURE": "/%postname%/"
